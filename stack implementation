class stack:
    def __init__(self,x,capacity):
        print(f"This is for object number {x} with a total capacity of {capacity} elements")
        self.lst = []
        self.capacity = capacity
        self.object = x
        
    def push(self,value):
            if len(self.lst)==self.capacity:
                print(f"the stack is full dude ..you cant insert further elements {value}")
            else:
                self.lst.append(value)

    def pop(self):
        return self.lst.pop()
    def prntstack(self):
        return self.lst

    def is_empty(self):
        if len(self.lst)==0:
            return True
        else:
            return False
    def peek(self):
        return self.lst[-1]
