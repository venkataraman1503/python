#without using sorted function
from array import *
myinput = array('i',[4,2,36,6,3])
for i in range(0,5):
    for j in range(1,5):
        if i<j and myinput[i]>myinput[j]:
            myinput[i],myinput[j]=myinput[j],myinput[i]
print(myinput)


#using sorted function
from array import *
myinput = array('i',[4,2,36,6,3])
print(sorted(myinput))
