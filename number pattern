for i in range(1,6):
    if i==1:
        print(i,end="")
        print()
        continue
    elif i>1:
        for j in range(1,i+1):
            print(j,end="")
            if j==i:
                for k in range(j-1,0,-1):
                    print(k,end="")
                print()
            else:
                continue
